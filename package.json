{
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "build": "turbo run build",
    "format": "prettier --write .",
    "format:check": "prettier --list-different \"{src,test,docs}/**/*.js\"",
    "dev": "turbo run dev --filter=@rsuite/charts",
    "docs": "turbo run build-docs --filter=@rsuite/charts",
    "publish-docs": "node docs/gh-pages.js",
    "prepages": "npm run docs",
    "pages": "npm run publish-docs",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "test": "jest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@swc-node/jest": "^1.5.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.omit": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.42",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^1.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "tsup": "^6.1.2",
    "turbo": "^1.3.1",
    "typescript": "^4.7.3"
  }
}
